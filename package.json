{
  "name": "mb-admin",
  "version": "1.3.0",
  "description": "一套基于 vue3、element-plus、typesScript、vite 的后台集成方案。",
  "author": "ShanYi-Hui <shanyi.hui@qq.com> (https://github.com/ShanYi-Hui)",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --mode base",
    "build": "run-p type-check \"build-only -- --mode {1}\" --",
    "preview": "vite preview --mode",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test:unit": "vitest",
    "prepare": "husky",
    "plop": "plop",
    "release": "standard-version"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.2",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "@zxcvbn-ts/core": "^3.0.4",
    "axios": "^1.7.2",
    "cropperjs": "^1.6.2",
    "driver.js": "^1.3.1",
    "echarts": "^5.5.1",
    "echarts-wordcloud": "^2.1.0",
    "element-plus": "^2.7.6",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qrcode": "^1.5.3",
    "qs": "^6.12.1",
    "vue": "^3.4.30",
    "vue-draggable-plus": "^0.5.0",
    "vue-json-pretty": "^2.4.0",
    "vue-router": "^4.4.0",
    "vue-types": "^5.1.2",
    "xgplayer": "^3.0.18",
    "monaco-editor": "^0.50.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.9",
    "@types/nprogress": "^0.2.3",
    "@types/qrcode": "^1.5.5",
    "@types/qs": "^6.9.15",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.0",
    "plop": "^4.0.1",
    "postcss-html": "^1.7.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "stylelint": "^16.6.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.5.2",
    "unocss": "^0.61.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.1",
    "vite-plugin-ejs": "^1.7.0",
    "vite-plugin-mock": "2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.22"
  },
  "packageManager": "npm@8.19.2",
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syh-micro-build/mb-admin.git"
  },
  "bugs": {
    "url": "https://github.com/syh-micro-build/mb-admin/issues"
  },
  "homepage": "https://github.com/syh-micro-build/mb-admin",
  "license": "MIT"
}
